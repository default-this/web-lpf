{
  "name": "feature-not-found",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "modules/features/bluetooth-unavailable/src",
  "prefix": "feat-not-found",
  "tags": ["type:feature","scope:not-found"],
  "projectType": "library",
  "targets": {
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "modules/features/not-found/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "modules/features/not-found/**/*.ts",
          "modules/features/not-found/**/*.html",
          "modules/features/not-found/package.json"
        ]
      }
    }
  }
}
