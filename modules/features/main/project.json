{
  "name": "feature-main",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "modules/features/main/src",
  "prefix": "feat-main",
  "tags": ["type:feature", "scope:main"],
  "projectType": "library",
  "targets": {
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "modules/features/main/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "modules/features/main/**/*.ts",
          "modules/features/main/**/*.html",
          "modules/features/main/package.json"
        ]
      }
    }
  }
}
